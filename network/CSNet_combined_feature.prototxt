name: "CSNet_combined_feature"
layer {
  name: "multi_data"
  type: "HDF5Data"
  top: "data"
  top: "label"
  include {
    phase: TRAIN
  }

  hdf5_data_param {
    source: "D:/likaiwen/network_LIVE/network3_4_2/train_1.txt"  #txt文件里保存多个hdf5文件的路径，一个hdf5文件路径占一行
    batch_size: 64

  }
}

layer {
  name: "conv1_1"
  type: "Convolution"
  bottom: "data"
  top: "conv1_1"
  param {
    lr_mult: 1
	decay_mult: 1
  }
  param {
    lr_mult: 2
  }
  convolution_param {
    num_output: 32
    kernel_size: 3
    stride: 1
	pad: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
	  value:0.2
    }
  }
}
layer {
    bottom: "conv1_1"
    top: "batch1_1"
    name: "batch1_1"
    type: "BatchNorm"
    batch_norm_param {
        use_global_stats: false
    }
}
layer {
  name: "relu1_1"
  type: "ReLU"
  bottom: "batch1_1"
  top: "relu1_1"
}

layer {
  name: "conv2_1"
  type: "Convolution"
  bottom: "relu1_1"
  top: "conv2_1"
  param {
