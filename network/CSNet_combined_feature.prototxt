##################
layer {
  name: "pool1"
  type: "Pooling"
  bottom: "relu2_1"
  top: "pool1"
  pooling_param {
    pool: AVE
    kernel_size: 100
    stride: 1
  }
}
layer {
  name: "pool2"
  type: "Pooling"
  bottom: "relu4_1"
  top: "pool2"
  pooling_param {
    pool: AVE
    kernel_size:  50
    stride: 1
  }
}
layer {
  name: "pool3"
  type: "Pooling"
  bottom: "relu6_1"
  top: "pool3"
  pooling_param {
    pool: AVE
    kernel_size: 25
    stride: 1
  }
}
layer {
  name: "pool4"
  type: "Pooling"
  bottom: "relu8_1"
  top: "pool4"
  pooling_param {
    pool: AVE
    kernel_size: 12
    stride: 1
  }
}
layer {
  name:"concat"
  bottom:"pool1"
  bottom:"pool2"
  bottom:"pool3"
  bottom:"pool4"
  top:"concat"
  type:"Concat"
  concat_param:{
    axis:1
  }
}
##################
layer {
  name: "ip1_1"
  type: "InnerProduct"
  bottom: "concat"
  top: "ip1_1"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  inner_product_param {
    num_output: 1000
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "relu9_1"
  type: "ReLU"
  bottom: "ip1_1"
  top: "relu9_1"
}
layer {
  name: "ip_feature_1"
  type: "InnerProduct"
  bottom: "relu9_1"
  top: "ip_feature_1"
