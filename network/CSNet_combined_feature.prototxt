}
layer {
  name: "pool1_1"
  type: "Pooling"
  bottom: "relu2_1"
  top: "pool1_1"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
layer {
  name: "conv3_1"
